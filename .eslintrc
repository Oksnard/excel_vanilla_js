{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "semi": ["error", "never"],
    "quotes": [2, "single", { "avoidEscape": true }]
  },
  "extends": ["eslint:recommended", "google"]
}